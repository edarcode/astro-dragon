---
import { Image } from "astro:assets";
import logo from "./dragon.avif";
import { ROUTER } from "../../../pages/rotuer";
import type { HTMLAttributes } from "astro/types";

interface Props extends HTMLAttributes<"a"> {}

const { class: cls, ...rest } = Astro.props;
---

<a
  href={ROUTER.index.path}
  class:list={["rounded-full", "overflow-hidden", cls]}
  {...rest}><Image src={logo} width={64} alt="logo dragon" /></a
>
